{
  "name": "@elzup/kit",
  "version": "1.3.8",
  "description": "elzup&#39;s utils",
  "access": "public",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "elzup/kit-js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "deploy": "npm run build && yarn publish",
    "lint": "eslint --ext .ts,.tsx",
    "test": "yarn test:jest && yarn test:unimported",
    "test:jest": "jest src",
    "test:unimported": "unimported",
    "test:cov": "jest src --coverage --ci --runInBand",
    "prepare": "husky install",
    "release": "yarn build && yarn publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "git add"
    ]
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "utils",
    "personal"
  ],
  "devDependencies": {
    "@swc/core": "1.2.135",
    "@swc/jest": "0.2.17",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "babel-jest": "27.4.6",
    "eslint": "8.8.0",
    "eslint-config-nzap": "5.0.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.3.2",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  }
}
