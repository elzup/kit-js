{
  "name": "@elzup/kit",
  "version": "4.5.0",
  "description": "elzup&#39;s utils",
  "access": "public",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "elzup/kit-js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "deploy": "npm run build && yarn publish",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "format": "prettier --check 'src/**/*.{ts,tsx}'",
    "format:fix": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "yarn test:jest && yarn test:unimported",
    "test:unused": "npx ts-unused-exports tsconfig.json --ignoreTestFiles",
    "test:jest": "jest src --maxWorkers=2",
    "test:unimported": "unimported",
    "test:cov": "jest src --coverage --maxWorkers=2",
    "test:badges": "yarn test:cov && istanbul-badges-readme",
    "prepare": "husky",
    "depcruise": "depcruise --config .dependency-cruiser.js --include-only '^src' --output-type dot src | dot -T svg > dependencygraph.svg"
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "git add"
    ]
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "utils",
    "personal"
  ],
  "devDependencies": {
    "@elzup/tsconfig": "1.1.0",
    "@swc/core": "1.14.0",
    "@swc/jest": "0.2.39",
    "@types/jest": "30.0.0",
    "@types/node": "24.9.2",
    "babel-jest": "30.2.0",
    "dependency-cruiser": "17.2.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "lint-staged": "16.2.6",
    "oxlint": "^1.25.0",
    "prettier": "^3.6.2",
    "ts-jest": "29.4.5",
    "tsd": "0.33.0",
    "typescript": "5.9.3",
    "unimported": "1.31.1"
  }
}
